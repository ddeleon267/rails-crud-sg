<%= form_for @beer do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :style %>
    <%= f.text_field :style %>
    <br>
    <%= f.label :brand %>
    <%= f.text_field :brand %>
    <br>
    <%= f.label :alcohol %>
    <%= f.text_field :alcohol %>
    <%= f.submit "Edit Beer" %>
<% end%>


